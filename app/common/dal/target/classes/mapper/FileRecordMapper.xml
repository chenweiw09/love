<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.book.xw.common.dal.dao.FileRecordMapper">
    <resultMap id="BaseResultMap" type="com.book.xw.common.dal.model.FileRecordDo">
        <result column="id" property="id" jdbcType="BIGINT"/>
        <result column="origin_file_name" property="originFileName" jdbcType="VARCHAR"/>
        <result column="file_name" property="fileName" jdbcType="VARCHAR"/>
        <result column="file_size" property="fileSize" jdbcType="DOUBLE"/>
        <result column="dir_path" property="dirPath" jdbcType="VARCHAR"/>
        <result column="desc" property="desc" jdbcType="VARCHAR"/>
        <result column="gmt_create" property="gmtCreate" jdbcType="TIMESTAMP"/>
        <result column="gmt_modified" property="gmtModified" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column">
        id, origin_file_name, file_name, file_size, dir_path, desc, gmt_create, gmt_modified
    </sql>

    <insert id="insert" parameterType="com.book.xw.common.dal.model.FileRecordDo" >
        insert into file_record (origin_file_name, file_name, file_size, dir_path, `desc`, gmt_create, gmt_modified)
        values(
        #{originFileName, jdbcType=VARCHAR},
        #{fileName, jdbcType=VARCHAR},
        #{fileSize, jdbcType=DOUBLE},
        #{dirPath, jdbcType=VARCHAR},
        #{desc, jdbcType=VARCHAR},
        #{gmtCreate, jdbcType=TIMESTAMP},
        #{gmtModified, jdbcType=TIMESTAMP}
        )
    </insert>



</mapper>